{
  "name": "theme",
  "displayName": "VSCODE DARKMODE",
  "description": "change vs code theme related to time ",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.48.0"
  },
  "publisher": "granddaleTech",
  "icon": "vscode_darkmode_extension.png",
  "categories": [
    "Visualization"
  ],
  "repository": {},
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "",
        "displayName": "",
        "selector": [
          {
            "filenamePattern": ""
          }
        ]
      }
    ],
    "configuration": {
      "title": "VSCODE DARKMODE",
      "properties": {
        "vscode_darkmode.dark": {
          "type": "string",
          "default": "Visual Studio Dark",
          "description": "define dark theme"
        },
        "vscode_darkmode.light": {
          "type": "string",
          "default": "Visual Studio Light",
          "description": "define light theme."
        },
        "vscode_darkmode.day": {
          "type": "number",
          "default": 4,
          "description": "defined time to change themes."
        },
        "vscode_darkmode.night": {
          "type": "number",
          "default": 19,
          "description": "defined time to change themes."
        }
      }
    },
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.48.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-open": "^3.0.1",
    "mocha": "^8.0.1",
    "os": "^0.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "vsce": "^1.79.5"
  }
}
